                                                            <label>Hora Salida</label>
                                                            <input type="text" class="form-control input-sm" id="horaSalidaEdit" name="horaSalidaEdit" />
                                                        </div>
                                                        <br />
                                                        <br />
                                                        <div class="col-md-12">
                                                            <label>Hora Regreso</label>
                                                            <input type="text" class="form-control input-sm" id="horaRegresoEdit" name="horaRegresoEdit" />
                                                        </div>
                                                        <br />
                                                        <br />
                                                        <div class="col-md-12">
                                                            <label>Duracion</label>
                                                            <input type="text" class="form-control input-sm" id="duracionEdit" name="duracionEdit" />
