<script type="text/javascript">
    function tourEdita(idTour, tour, descripcion, origen, destino, horaSalida, horaRegreso, duracion) {
        idTourEdit = idTour;
        document.getElementById("idTourEdit").value = idTour;
        document.getElementById("tourEdit").value = tour;
        document.getElementById("descripcionEdit").value = descripcion;
        document.getElementById("origenEdit").value = origen;
        document.getElementById("destinoEdit").value = destino;
        document.getElementById("horaSalidaEdit").value = horaSalida;
        document.getElementById("horaRegresoEdit").value = horaRegreso;
        document.getElementById("duracionEdit").value = duracion;
        $('#modal-tour').modal('show');
    }
    function EditaTours() {
        $("#frmEditarTour").submit();
    }
    //------------------------------------------------------------------------------------------------------------------------------------
    function agenciaEdita(idAgencia, nombre, direccion, descripcion) {
        idTourEdit = idTour;
        document.getElementById("idAgenciaEdit").value = idAgencia;
        document.getElementById("nombreEdit").value = nombre;
        document.getElementById("direccionEdit").value = direccion;
        document.getElementById("descripcionEditA").value = descripcion;
        $('#modal-agencia').modal('show');
    }
    function EditaAgencias() {
        $("#frmEditarAgencia").submit();
    }
    //--------------------------------------------------------------------------------------------------------------------------------------
    function precioEdita(idPrecio, nombre, nombreTour, precioALta, precioBaja) {
        idTourEdit = idTour;
        document.getElementById("idPrecioEdit").value = idPrecio;
        document.getElementById("nombreEditP").value = nombre;
