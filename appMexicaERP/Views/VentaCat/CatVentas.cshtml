                                CATALOGO PRECIOS
                                @*<small>Cancela venta(Tour o Pquete)</small>*@
                            </li>
                        </ol>
                        <!-- begin wizard step-1 -->
                        <div>
                            <form action="@Url.Action("InsertarCatTour", "VentaCat" )" method="POST">
