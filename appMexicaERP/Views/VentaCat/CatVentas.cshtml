                                            </div>
                                            <div class="col-md-4">
                                                <label>Descripción</label>
                                                <input type="text" class="form-control input-sm" id="descripcion" name="descripcion" />
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end row -->
                                    <br />
                                    <!-- begin row -->
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-1">
                                            </div>
