                                                <input type="text" class="form-control input-sm" id="origen" name="origen" />
                                            </div>
                                            <div class="col-md-3">
                                                <label>Destino Tour</label>
                                                <input type="text" class="form-control input-sm" id="destino" name="destino" />
                                            </div>
                                            <div class="col-md-1">
                                                <label>H Salida</label>
                                                <input type="text" class="form-control input-sm" id="horaSalida" name="horaSalida" />
                                            </div>
                                            <div class="col-md-1">
                                                <label>H Regreso</label>
                                                <input type="text" class="form-control input-sm" id="horaRegreso" name="horaRegreso" />
                                            </div>
                                            <div class="col-md-2">
                                                <label>Duracion Horas</label>
                                                <input type="text" class="form-control input-sm" id="duracion" name="duracion" />
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end row -->
                                    <br />
                                    <!-- begin row -->
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-1">

                                            </div>
                                            <div class="col-md-2">
                                                <button type="submit" class="btn btn-primary btn-sm m-r-5">Agregar Tour</button>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end row -->
                                    <br />
                                    <br />
                                    <div class="row">
                                        <div class="form-group">
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-10">
                                                <table id="data-table_tour" class="table table-striped table-bordered">
                                                    <thead>
                                                        <tr style="width:10% !important">
                                                            <th style='text-align:center !important; font-style:normal; font-size :13pt;  width:55% !important;' class='sorting' tabindex='0' aria-controls='searchable' rowspan='1' colspan='1' aria-label='Rendering engine: activate to sort column ascending'>TOUR </th>
                                                            <th style='text-align:center !important; font-style:normal; font-size :13pt;' class='sorting' tabindex='0' aria-controls='searchable' rowspan='1' colspan='1' aria-label='Rendering engine: activate to sort column ascending'>DETALLE TOUR </th>
                                                            <th style='text-align:center !important; font-style:normal; font-size :13pt;' !important;' class='sorting' tabindex='0' aria-controls='searchable' rowspan='1' colspan='1' aria-label='Rendering engine: activate to sort column ascending'>EDICIÓN</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="body-data-table">

                                                        @foreach (var item in ViewBag.listaTour)
                                                        {

                                                            <tr>
                                                                <td style="text-align:center !important; font-family:'Times New Roman', Times, serif; font-size:10pt;">@item.tour</td>
                                                                <td style="text-align:center !important; font-family:'Times New Roman', Times, serif; font-size:10pt;">@item.descripcion</td>
                                                                <td style="text-align:center !important; font-family:'Times New Roman', Times, serif; font-size:10pt;">
                                                                    <a class="btn btn-primary btn-icon btn-circle btn-lg" onclick="tourEdita('@item.idTour', '@item.tour','@item.descripcion', '@item.origen','@item.destino', '@item.horaSalida','@item.horaRegreso', '@item.duracion');"><i class="fa fa-automobile"></i></a>
                                                                    <a class="btn btn-danger btn-icon btn-circle btn-lg" onclick="EliminaTour('@item.idTour');"><i class="fa fa-times"></i></a>
                                                                </td>
                                                            </tr>
                                                        }
                                                    </tbody>
                                                </table>

                                            </div>
                                        </div>

                                    </div>
                                </fieldset>
                            </form>
                            <div class="modal fade" id="modal-tour">
                                <form action="@Url.Action("EditaTour", "VentaCat" )" id="frmEditarTour" method="POST">
