
@{
    ViewBag.Title = "Registrar";
}
<div class="container">
    <h2>Registro de Identificaciones validas</h2>
    <div class="row">
        <div class="col-md-12">
            <!-- begin panel -->
            <div class="panel panel-inverse" data-sortable-id="form-stuff-1">
                <div class="panel-heading">
                    <h4 class="panel-title">Registro de Identificaciones</h4>
                </div>
                <div class="panel-body">
                    <form action="@Url.Action("Registrar", "TipoIndentificacion")" class="form-horizontal" method="post">
                        <div class="form-group">
                            <label class="col-md-1 control-label" align="left">Descripcion</label>
                            <div class="col-md-3">
                                <input type="text" onKeyUp="this.value=this.value.toUpperCase();" id="txtdescripcion" name="txtdescripcion" class="form-control" placeholder="Nombre de la categoria" required />
                            </div>
                        </div>
                        <div class="form-group col-md-12">
                            <div class="col-md-12" align="center">
                                <button style="width:10%;" type="submit" class="btn btn-sm btn-success">Insertar</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <!-- end panel -->
        </div>
    </div>
</div>

